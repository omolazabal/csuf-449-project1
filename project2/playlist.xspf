<?xml version="1.0" encoding="UTF-8"?>
<playlist version="1" xmlns="http://xspf.org/ns/0/">
	<trackList>
	{% for url in urls %}
		<track>
			<location>{{ url }}</location>

			<creator></creator>
			<album></album>
			<title></title>
			<annotation></annotation>
			<duration></duration>
			<image></image>
		</track>
	{% endfor %}
	</trackList>
</playlist>