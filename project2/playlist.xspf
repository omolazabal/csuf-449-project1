<?xml version="1.0" encoding="UTF-8"?>
<playlist version="1" xmlns="http://xspf.org/ns/0/">

    <title>{{ title }}</title>

    <creator>{{ creator }}</creator>

    <info>{{ description }}</info>

	<trackList>
	{% for url in urls %}
		<track>
			<location>{{ url }}</location>
		</track>
	{% endfor %}
	</trackList>
</playlist>